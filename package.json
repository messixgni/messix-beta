    "moribuild": "mv ./public/analytics-override.js ./public/analytics-override.default&&mv ./public/analytics-override.mori ./public/analytics-override.js&&mv ./src/ga/ga.ts ./src/ga/ga.dev&&mv ./src/ga/ga.production ./src/ga/ga.ts&&webpack --config webpack/webpack.prod.js&&mv ./src/ga/ga.ts ./src/ga/ga.production&&mv ./src/ga/ga.dev ./src/ga/ga.ts&&mv ./public/analytics-override.js ./public/analytics-override.mori&&mv ./public/analytics-override.default ./public/analytics-override.js",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chibat/chrome-extension-typescript-starter.git"
