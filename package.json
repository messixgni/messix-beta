    "bootstrap": "^5.1.3",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "react": "^17.0.1",
    "react-bootstrap": "^2.3.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.158",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^27.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2 ",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.0",
    "typescript": "^4.4.3 ",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0"
  }
